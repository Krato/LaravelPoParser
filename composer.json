{
    "name": "EricLagarda/LaravelPoParser",
    "description": "Laravel PoParser",
    "keywords": [
        "laravel5",
        "po parser"
    ],
    "homepage": "https://github.com/krato/LaravelPoParser",
    "license": "MIT",
    "authors": [
        {
            "name": "EricLagarda",
            "email": "info@ericlagarda.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0",
        "gettext/gettext": "3.*",
        "sepia/po-parser" : "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "EricLagarda\\LaravelPoParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EricLagarda\\LaravelPoParser\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
